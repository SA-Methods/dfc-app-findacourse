{
  "APPINSIGHTS_INSTRUMENTATIONKEY": "<<Key Here>>",
  "Logging": {
    "LogLevel": {
      "Default": "Error"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "Configuration": {
    "ApplicationName": "DFC-App-Find-a-course",
    "CosmosDbConnections": {
      "ContentPages": {
        "AccessKey": "[KeyHere]",
        "EndpointUrl": "https://localhost:8081",
        "DatabaseId": "dfc-app-conpage",
        "CollectionId": "pages",
        "PartitionKey": "/PartitionKey"
      },
      "StaticContent": {
        "AccessKey": "[Key]",
        "EndpointUrl": "https://localhost:8081",
        "DatabaseId": "dfc-shared-content",
        "CollectionId": "shared-content",
        "PartitionKey": "/PartitionKey"
      }
    },
    "CourseSearchClient": {
      "CosmosAuditConnection": {
        "AccessKey": "[Key]",
        "EndpointUrl": "https://localhost:8081",
        "DatabaseId": "dfc-digital-audit",
        "CollectionId": "CourseSearchAuditRecords",
        "PartitionKey": "/PartitionKey"
      },
      "CourseSearchSvc": {
        "ServiceEndpoint": "https://dev.api.nationalcareersservice.org.uk/coursedirectory/findacourse/",
        "APIKey": "[Key]",
        "SearchPageSize": 20,
        "RequestTimeOutSeconds": 10,
        "TransientErrorsNumberOfRetries": 3
      },
      "Policies": {
        "HttpCircuitBreaker": {
          "DurationOfBreak": "00:00:10",
          "ExceptionsAllowedBeforeBreaking": 3
        },
        "HttpRetry": {
          "BackoffPower": 2,
          "Count": 3
        }
      }
    }
  },
  "SubscriptionSettings": {
    "Endpoint": "https://localhost/api/webhook/ReceiveEvents",
    "SubscriptionServiceEndpoint": "https://localhost/api/execute/",
    "SubscriptionRegistrationDelay": "00:02:00",
    "Filter": {
      "IncludeEventTypes": [ "published", "unpublished", "deleted" ],
      "PropertyContainsFilters": [
        {
          "Key": "subject",
          "Values": [
            "/content/sharedcontent/"
          ]
        }
      ]
    }
  },
  "CmsApiClientOptions": {
    "Timeout": "00:00:30",
    "ApiKey": "",
    "BaseAddress": "https://dfc-dev-api-cont-fa.azurewebsites.net/",
    "SummaryEndpoint": "api/execute/jobprofile",
    "StaticContentEndpoint": "api/execute/sharedcontent/",
    "ContentIds": "531ed13a-5659-4a36-a822-9f4795b7b1c8"
  },
  "EventGridSubscriptionClientOptions": {
    "Timeout": "00:00:30",
    "ApiKey": "[APIM key]",
    "BaseAddress": "https://localhost/",
    "Endpoint": "api/execute/"
  },
  "EventGridSubscriptionModel": {
    "Endpoint": "https://localhost/api/webhook/ReceiveEvents",
    "Filter": {
      "BeginsWith": "/jobprofile/",
      "IncludeEventTypes": [ "published", "unpublished", "deleted" ]
    }
  }
}